	month = jul,
	year = {2011},
	note = {{PMID:} 21576222},
	pages = {e90--e90},
	file = {Full Text PDF:/home/julian/embl/paper/storage/TR3XK768/Nakamura et al. - 2011 - Sequence-specific error profile of Illumina sequen.pdf:application/pdf;Snapshot:/home/julian/embl/paper/storage/7M8VIGUP/e90.html:text/html}
