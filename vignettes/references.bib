	keywords = {Animals, Base Sequence, Dog Diseases, Dogs, Gene Expression Regulation, Phylogeny, Point Mutation, Tumor Suppressor Protein p53, Venereal Tumors, Veterinary},
	pages = {187--191}
},

@article{leek_tackling_2010,
