	author = {Navin, Nicholas and Kendall, Jude and Troge, Jennifer and Andrews, Peter and Rodgers, Linda and {McIndoo}, Jeanne and Cook, Kerry and Stepansky, Asya and Levy, Dan and Esposito, Diane and Muthuswamy, Lakshmi and Krasnitz, Alex and {McCombie}, W. Richard and Hicks, James and Wigler, Michael},
