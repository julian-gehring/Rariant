@article{bautista-gomez_analysis_2011,
	title = {Analysis of canine transmissible veneral tumor genotypes using the D-loop region of mitochondrial {DNA}},
	volume = {86},
	issn = {1880-5779},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/22362033},
	abstract = {Canine transmissible venereal tumor ({CTVT)} is the only neoplasm that can be spread among dogs through cell transplantation. Therefore, this tumor does not originate from host cell transformation. Although {CTVT} has a monophyletic origin, several studies have shown the presence of genetic diversity which was probably acquired after the development of its original clone. To investigate the genetic diversity of {CTVT} in Mexico and its relation with {CTVTs} disseminated worldwide, we sequenced a fragment of mitochondrial {DNA} in 50 tumor samples and matched blood samples from dog hosts from Mexico. We found ten new haplotypes in tumor samples, which were all distinct from their matched host. The {TVT1} haplotype was the most frequent in our samples, suggesting that it could be the origin of the others. We found that haplotypes in Mexico and other countries are distributed in two well-defined clusters. Our data also suggest a close relationship among American haplotypes (Mexico, {USA}, Chile and Brazil). Interestingly, these American haplotypes were also closely related to Asian haplotypes. Taking into account the estimated timing of the origin of {CTVT}, we propose that {CTVT} might have originated in Asia; consequently, haplotypes currently present in America could descend from Asiatic lineages.},
	number = {5},
	urldate = {2012-08-01},
	journal = {Genes \& genetic systems},
	author = {Bautista-Gómez, Linda G and Martínez-Castañeda, Simón and Córdova-Alarcón, Emilio and Vázquez-Chagoyán, Juan Carlos},
	year = {2011},
	note = {{PMID:} 22362033},
	keywords = {Animals, Base Sequence, {DNA}, Mitochondrial, Dog Diseases, Dogs, Genotype, Haplotypes, Locus Control Region, Mexico, Molecular Sequence Data, Phylogeny, Polymorphism, Single Nucleotide, Sequence Analysis, {DNA}, Venereal Tumors, Veterinary},
	pages = {351--355}
},

