	urldate = {2012-08-01},
	journal = {Journal of vascular and interventional radiology: {JVIR}},
	author = {Ahrar, Kamran and Madoff, David C and Gupta, Sanjay and Wallace, Michael J and Price, Roger E and Wright, Kenneth C},
	month = sep,
	year = {2002},
	note = {{PMID:} 12354827},
	keywords = {Animals, Dogs, Lung Neoplasms, Models, Animal, Neoplasm Transplantation, Venereal Tumors, Veterinary},
	pages = {923--928}
},

