	year = {2013},
	note = {{PMID:} 23539594},
	pages = {1546--1558},
	file = {Full Text PDF:/home/julian/embl/paper/storage/HVMD46KF/Vogelstein et al. - 2013 - Cancer Genome Landscapes.pdf:application/pdf;Snapshot:/home/julian/embl/paper/storage/P227G9DK/1546.html:text/html}
},

